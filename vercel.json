{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "main.py": {
      "maxDuration": 300
    }
  },
  "build": {
    "env": {
      "SUPABASE_URL": "@supabase_url",
      "SUPABASE_KEY": "@supabase_key"
    }
  }
}